<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="commonLibaryAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/sintn_colorprimary_color</item>
        <item name="colorPrimaryDark">@color/sintn_colorprimarydark_color</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="commonLibaryAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="commonLibaryAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="commonLibaryAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="background">@color/sintn_colorprimary_color</item>
        <item name="android:background">@color/sintn_colorprimary_color</item>
        <item name="backgroundSplit">@color/black</item>
        <item name="android:backgroundSplit">@color/black</item>
        <item name="android:elevation" tools:ignore="NewApi">0dp</item>
    </style>


    <style name="CustomDialogStyle" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Dialog_Anim">
        <item name="android:windowEnterAnimation">@anim/fading_in</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="info_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
        <item name="android:windowAnimationStyle">@style/Dialog_Anim</item>
    </style>

    <style name="backgroundDimDialog" parent="@android:style/Theme.Dialog">

        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="backgroundTransparentDialog" parent="@android:style/Theme.Dialog">

        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:backgroundDimAmount">0.0</item>
    </style>

    <style name="base_style_for_dialogFragment" parent="@style/backgroundTransparentDialog">
        <item name="android:layout_margin">100dp</item>
    </style>

    <!-- 水平直线 -->
    <style name="horizontalline">
        <item name="android:background">#ABAE9E</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dip</item>
    </style>

    <style name="mystyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter1</item>
        <!-- 进入时的动画 -->
        <item name="@android:windowExitAnimation">@anim/dialog_exit</item>
        <!-- 退出时的动画 -->
    </style>

    <style name="mystyle_translation_from_header_to_bottom_animation" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter</item>
        <!-- 进入时的动画 -->
        <item name="@android:windowExitAnimation">@anim/dialog_exit</item>
        <!-- 退出时的动画 -->
    </style>

    <style name="mystyle_translation_horizontal_animation" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/enter_lefttoright</item>
        <!-- 进入时的动画 -->
        <item name="@android:windowExitAnimation">@anim/exit_righttoleft</item>
        <!-- 退出时的动画 -->
    </style>

    <style name="mystyle_translation_horizontal_animation_right_to_left" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/enter_righttoleft</item>
        <!-- 进入时的动画 -->
        <item name="@android:windowExitAnimation">@anim/exit_lefttoright</item>
        <!-- 退出时的动画 -->
    </style>

    <style name="mystyle_scale" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/scal_popupwindow_enter</item>
        <!-- 进入时的动画 -->
        <item name="@android:windowExitAnimation">@anim/scal_popupwindow_exit</item>
        <!-- 退出时的动画 -->
    </style>

    <style name="tv_performance_item">
        <item name="android:textColor">@color/c333333</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/margin_of_listitem_tv</item>
        <item name="android:layout_marginRight">@dimen/margin_of_listitem_tv</item>
    </style>

    <style name="tv_performance_itemName" parent="tv_performance_item">
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textColor">@color/text_left_color</item>
    </style>

    <style name="tv_performance_itemValue" parent="tv_performance_item">
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="tv_info_item">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/text_left_color</item>
        <item name="android:paddingTop">@dimen/info_item_margin_vertical</item>
        <item name="android:paddingBottom">@dimen/info_item_margin_vertical</item>
        <item name="android:textSize">@dimen/normal_textSize</item>
    </style>

    <style name="tv_info_item_name" parent="tv_info_item">
        <item name="android:layout_marginLeft">@dimen/info_item_margin_horizontal</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="tv_info_item_value" parent="tv_info_item">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">@color/text_right_color</item>
        <item name="android:layout_marginRight">@dimen/info_item_margin_horizontal</item>
    </style>


